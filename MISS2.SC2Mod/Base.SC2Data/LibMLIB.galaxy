bool libMLIB_gt_ReplaceDoodads_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int lv_player;
    region lv_doodadsRegion;
    string lv_faction;
    string lv_factionDoodadsRace;
    string lv_factionObjectsRace;

    // Automatic Variable Declarations
    playergroup auto03781FCD_g;

    // Variable Initialization
    lv_doodadsRegion = RegionFromName("Doodads Replacement Area");
    lv_faction = libMLIB_gv_enemyFaction;
    lv_factionDoodadsRace = UserDataGetGameLink("Factions", lv_faction, "Doodads", 1);
    lv_factionObjectsRace = UserDataGetGameLink("Factions", lv_faction, "Objects", 1);

    // Actions
    if (!runActions) {
        return true;
    }

    libMLIB_gf_DoodadsReplacement(lv_factionDoodadsRace, lv_factionObjectsRace, lv_doodadsRegion);
    auto03781FCD_g = libMLIB_gf_GetEnemyPlayers();
    lv_player = -1;
    while (true) {
        lv_player = PlayerGroupNextPlayer(auto03781FCD_g, lv_player);
        if (lv_player < 0) { break; }
        lv_doodadsRegion = RegionFromName(("Doodads Replacement Area " + IntToString(lv_player)));
