void libMISS_gf_BelshirEscortConfig (fixed lp_delay, string lp_spawns, point lp_harvesterSpawnPoint) {
    // Variable Declarations
    int lv_nPCPlayer;
    int lv_player;
    point lv_point;
    unit lv_unit;
    unitgroup lv_units;

    // Automatic Variable Declarations
    int auto670E11FA_size;
    int auto670E11FA_i;

    // Variable Initialization
    lv_nPCPlayer = libARRA_gf_IntegerForTask2(libARRA_gv_task, "NPC-ALLY");
    lv_player = libARRA_gf_IntegerForTask(libARRA_gv_taskGlobal, "NPC-NEUTRAL");
    lv_units = UnitGroupEmpty();

    // Implementation
    libMISS_gf_BEVO();
    libARRA_gf_StorePointForTask(libARRA_gv_task, "BE-HARVESTER-SPAWN", PointFromName("BE-Spawn"));
    auto670E11FA_size = libARRA_gf_ListSize( lp_spawns);
    for (auto670E11FA_i = 1; auto670E11FA_i <= auto670E11FA_size; auto670E11FA_i += 1 ) {
        lv_point = libARRA_gf_ListPointGet(lp_spawns, auto670E11FA_i );
        UnitCreate(1, "TerrazineNodeOff", c_unitCreateIgnorePlacement, lv_player, lv_point, PointGetFacing(lv_point));
        lv_unit = UnitLastCreated();
        UnitSetState(lv_unit, c_unitStateSelectable, false);
        libMLIB_gf_SetUnitInfoTextCampaign(lv_unit, null, StringToText(""), null);
        UnitGroupAdd(lv_units, lv_unit);
        ;
    }
    libARRA_gf_StoreUnitGroupForTask(libARRA_gv_task, "BE-Nodes", lv_units);
    libARRA_gf_StoreIntegerForTask(libARRA_gv_task, "BE-ESCORT-LIFE", libMLIB_gf_DifficultyValueInt3(libARRA_gf_IntegerForTask2(libARRA_gv_task, "Difficulty"), 1500, 1500, 1250, 1000));
    CatalogFieldValueSet(c_gameCatalogBehavior, "DefensiveMatrixTerranzineHarvester", "Modification.VitalMaxArray[" + IntToString(1) + "]", lv_nPCPlayer, FixedToString(500.0, c_fixedPrecisionAny));
    CatalogFieldValueSet(c_gameCatalogBehavior, "DefensiveMatrixTerranzineHarvester", "Duration", lv_nPCPlayer, FixedToString(10.0, c_fixedPrecisionAny));
    libARRA_gf_StoreTriggerForTask(libARRA_gv_task, "Attack", libMISS_gt_BEEnemyReinforcements);
    libARRA_gf_StoreTriggerForTask(libARRA_gv_task, "Harass", libMISS_gt_BEEnemyHarass);
    libARRA_gf_StoreIntegerForTask(libARRA_gv_task, "Harvesters", 0);
    libARRA_gf_StoreIntegerForTask(libARRA_gv_task, "Waves", 0);
    libARRA_gf_StoreUnitGroupForTask(libARRA_gv_task, "Harvesters", UnitGroupEmpty());
    libARRA_gf_StoreIntegerForTask(libARRA_gv_task, "Goal", 0);
    libARRA_gf_Taskon(libARRA_gv_task, "Progress", libMISS_gt_BEEscortProgressHarvestersWaveFinished);
    libARRA_gf_Taskon(libARRA_gv_task, "Complete", libMISS_gt_BEEscortCompletedLastHarvesterWaveFinished);
    libARRA_gf_TaskObjective(libARRA_gv_task, "AC_BelshirEscort_Primary01", true);
    libARRA_gf_Taskon(libARRA_gv_task, "Start", libMISS_gt_BEEscortStart);
    libARRA_gf_RunMissionVictorySequence6(libARRA_gv_task, lp_delay, libARRA_gt_TaskStart);
}

